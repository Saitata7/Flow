{
  "name": "flow",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "start": "yarn workspace @flow/mobile start",
    "android": "yarn workspace @flow/mobile android",
    "ios": "yarn workspace @flow/mobile ios",
    "web": "yarn workspace @flow/mobile web",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "yarn": "^1.22.0"
  }
}
